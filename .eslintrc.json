{
    "plugins": ["react", "prettier"],
    "extends": ["react-app", "plugin:prettier/recommended", "prettier"],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true
        }
      ],
      "react/jsx-filename-extension": [
        "warn",
        {
          "extensions": [".js", ".jsx"]
        }
      ]
    }
  }